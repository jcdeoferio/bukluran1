<%= render :partial => '/osas/menu' %>
<td class = "content_area">
	<h2><hr>Search Result<hr></h2>
	<div class = "content">
	<% if @organizations %>
		Search result for "<%= params[:name]%>"
		<ul>
		<% for organization in @organizations %>
			<li> <%= link_to "#{organization.name} (#{organization.acronym})",  :controller => 'osas', :action => 'show_forms', :id => organization.id %>
		<% end %>
		</ul>
	<% else %>
		No match found for "<%= params[:name] %>"
	<% end %>
	</div>
</td>