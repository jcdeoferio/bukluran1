<%= render :partial => '/organizations/menu' %>

<td class = "content_area">
	<h2><hr>Awards Received<hr></h2>
	<%= link_to 'Add Award', :action => 'new' %>
	<% for award in @awards %>
		<h4><%=h award.name %></h4>
		<p>
			Classification: <%=h award.classification %> <br>
			Arward-Giving Body: <%=h award.from %> <br>
			Description: <%=h award.description %> <br>
		</p>
	<% end %>

</td>
