<%= render :partial => '/osas/menu' %>
<td class = "content_area">
	<h2><hr><%= "#{@data.name} Form 2" %><hr></h2>
		<strong><font color = 'red'><%= flash[:notice] %></font></strong>
		<ul>
			<% for filename in @files %>
				 <% pathname = "Form_2/#{filename}"%>
				 <%= image_tag("/data/#{@data.name}/#{@yearsem}/Form_2/#{filename}", :size => "100x100")  %><br>
				 <li><%= link_to "View #{filename} in Bigger Size", :action => 'view_form', :id => @data.id, :pname => pathname, :ysem => @yearsem %><br>
			<% end %>
			<br><br>
			<%= button_to "Back", :controller => 'osas', :action => 'show_forms', :id => @data.id %>
		<ul>
</td>